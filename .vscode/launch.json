    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug with delve",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "GOOS": "linux",
                "GOARCH": "amd64"
            },
            "args": [],
            "delve": {
                "preLaunchTask": "build"
            }
        },
        {
            "name": "Debug TestPostArticlesStart",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/cmd",
            "args": ["-test.run", "TestPostArticlesStart"],
            "env": {"GOFLAGS": "-v"}
        },
        {
            "name": "Launch Crawler Mid North Monitor",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceRoot}",
            "program": "cmd/crawler/main.go",
            "args": [
                "https://www.midnorthmonitor.com/category/news/local-news/",
                "b55fe232-0fbf-4fa8-b697-ff7bb863ae6a"
            ]
        },
        {
            "name": "Launch Crawler Sudbury",
